<h1>Listing cost_bills</h1>

<table>
  <tr>
    <th>Money</th>
    <th>Remarks</th>
    <th>Billdate</th>
    <th>Cost type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cost_bills.each do |cost_bill| %>
  <tr>
    <td><%= cost_bill.money %></td>
    <td><%= cost_bill.remarks %></td>
    <td><%= cost_bill.billdate %></td>
    <td><%= cost_bill.cost_type %></td>
    <td><%= link_to 'Show', cost_bill %></td>
    <td><%= link_to 'Edit', edit_cost_bill_path(cost_bill) %></td>
    <td><%= link_to 'Destroy', cost_bill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cost bill', new_cost_bill_path %>
